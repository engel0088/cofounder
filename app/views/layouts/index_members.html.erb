<!DOCTYPE html>
<html>
<head>
  <title>Cofounderlab</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= stylesheet_link_tag    "reset", :media => "all" %>
  <%= stylesheet_link_tag    "homestyle", :media => "all" %>
  <%= stylesheet_link_tag    "old_style_change", :media => "all" %>
  <%= stylesheet_link_tag    "profilecss", :media => "all" %>
  <%= stylesheet_link_tag    "supersized", :media => "all" %>
  <%= stylesheet_link_tag    "supersized_shutter", :media => "all" %>
  <%= stylesheet_link_tag    "themestyle", :media => "all" %>

  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>

<body>

   <div id="main_wrapper" class="main_wrapper">

      <!-- header part start -->
      <div id="header" class="header">
         <%= render :partial => 'shared/header' %>
      </div>
      <!-- header part end -->

      <!-- container start -->
      <div class="baseClass">
         <%= yield %>
      </div>
      <!-- containter end -->
   </div>
   <!-- main wrapper end -->


   <!-- footer start -->
   <div id="footer">
         <%= render :partial => 'shared/footer' %>
   </div>
   <!-- footer end -->
</body>
</html>
